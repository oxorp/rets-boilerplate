{
  "name": "rets-boilerplate",
  "version": "1.0.0",
  "description": "Basic boilerplate for reactJs apps. Featuring Typescript and Jest",
  "main": "index.tsx",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod",
    "dev": "webpack-dev-server --config=configs/webpack/dev",
    "prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "author": "Robert Hartkopf",
  "license": "WTFPL",
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/react-test-renderer": "^16.0.3",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.7.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
